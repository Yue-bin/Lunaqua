<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="using:Lunaqua.ViewModels"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:views="using:Lunaqua.Views"
        xmlns:components="using:Lunaqua.Components"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="Lunaqua.Views.MainWindow"
        x:Name="LunaquaWindow"
        x:DataType="vm:ThemeViewModel"
        RequestedThemeVariant="{Binding CurrentTheme}"
        Icon="/Assets/lunaqua-logo-main.png"
        Title="Lunaqua"
        TransparencyLevelHint="AcrylicBlur"
        Background="Transparent"
        Width="800"
        Height="450">

    <views:MainView />
</Window>
